                android:background="#00B0FF" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="#7AC4FF" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>